<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/cardReservaDetalleItem"
    android:layout_width="match_parent"
    android:layout_height="200dp"
    android:layout_margin="20dp"
    android:elevation="20dp"
    app:cardBackgroundColor="@color/rosa_para_input"
    app:cardCornerRadius="15dp">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_margin="5dp">

        <androidx.cardview.widget.CardView
            android:id="@+id/cardView10"
            android:layout_width="52dp"
            android:layout_height="53dp"
            android:layout_marginStart="55dp"
            android:layout_marginTop="36dp"
            android:layout_marginBottom="24dp"
            app:cardCornerRadius="50dp"
            app:layout_constraintBottom_toTopOf="@+id/fabLlenarFormulario"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <ImageView
                android:id="@+id/imageView17"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                tools:srcCompat="@drawable/avatar_1" />
        </androidx.cardview.widget.CardView>

        <TextView
            android:id="@+id/textView35"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginEnd="90dp"
            android:text="Servicio - para Nombre"
            android:textAlignment="textStart"
            android:textColor="#000000"
            android:textSize="14sp"
            app:layout_constraintBottom_toTopOf="@+id/fabLlenarFormulario"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/cardView10"
            app:layout_constraintTop_toTopOf="parent" />

        <com.google.android.material.floatingactionbutton.FloatingActionButton
            android:id="@+id/fabLlenarFormulario"
            android:layout_width="70dp"
            android:layout_height="55dp"
            android:layout_marginStart="76dp"
            android:layout_marginEnd="61dp"
            android:layout_marginBottom="16dp"
            android:clickable="true"
            app:backgroundTint="#6750A4"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/fabRechazarSolicitud"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/cardView10"
            app:srcCompat="@drawable/ic_write"
            app:tint="@android:color/white"
            tools:ignore="SpeakableTextPresentCheck" />

        <com.google.android.material.floatingactionbutton.FloatingActionButton
            android:id="@+id/fabRechazarSolicitud"
            android:layout_width="70dp"
            android:layout_height="55dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="22dp"
            android:layout_marginEnd="88dp"
            android:layout_marginBottom="16dp"
            android:clickable="true"
            app:backgroundTint="#FFFFFF"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/fabLlenarFormulario"
            app:layout_constraintTop_toBottomOf="@+id/cardView10"
            app:srcCompat="@drawable/ic_cancel"
            app:tint="#6750A4"
            tools:ignore="SpeakableTextPresentCheck" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.cardview.widget.CardView>